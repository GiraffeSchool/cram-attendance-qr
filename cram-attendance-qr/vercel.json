{
    "functions": {
      "api/index.js": {
        "maxDuration": 30
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      },
      {
        "source": "/attend",
        "destination": "/api/attend"
      },
      {
        "source": "/webhook",
        "destination": "/api/webhook"
      }
    ]
  }